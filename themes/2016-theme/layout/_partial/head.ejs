<%
var title = page.title;

if (is_archive()){
  title = 'Archives';

  if (is_month()){
    title += ': ' + page.year + '/' + page.month;
  } else if (is_year()){
    title += ': ' + page.year;
  }
} else if (is_category()){
  title = 'Category: ' + page.category;
} else if (is_tag()){
  title = 'Tag: ' + page.tag;
}
%>

<meta charset="utf-8" />
<!-- Begin faviconss -->
<link rel="apple-touch-icon-precomposed" sizes="57x57" href="<%- url_for('favicons/apple-touch-icon-57x57.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="114x114" href="<%- url_for('favicons/apple-touch-icon-114x114.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="72x72" href="<%- url_for('favicons/apple-touch-icon-72x72.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="144x144" href="<%- url_for('favicons/apple-touch-icon-144x144.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="60x60" href="<%- url_for('favicons/apple-touch-icon-60x60.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="120x120" href="<%- url_for('favicons/apple-touch-icon-120x120.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="76x76" href="<%- url_for('favicons/apple-touch-icon-76x76.png') %>" />
<link rel="apple-touch-icon-precomposed" sizes="152x152" href="<%- url_for('favicons/apple-touch-icon-152x152.png') %>" />
<link rel="icon" type="image/png" href="<%- url_for('favicons/favicon-196x196.png') %>" sizes="196x196" />
<link rel="icon" type="image/png" href="<%- url_for('favicons/favicon-96x96.png') %>" sizes="96x96" />
<link rel="icon" type="image/png" href="<%- url_for('favicons/favicon-32x32.png') %>" sizes="32x32" />
<link rel="icon" type="image/png" href="<%- url_for('favicons/favicon-16x16.png') %>" sizes="16x16" />
<link rel="icon" type="image/png" href="<%- url_for('favicons/favicon-128.png') %>" sizes="128x128" />
<!-- End faviconss -->
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

<title><% if (title){ %><%= strip_html(title) %> | <% } %><%= strip_html(config.title) %></title>

<meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />

<!-- Fonts and icons -->
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
  <%- css('https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css') %>
<!-- End Fonts and icons -->

<!-- CSS Files -->
  <%- css('css/bootstrap.min') %>
	<%- css('css/material-kit') %>
<!-- End CSS Files -->

<!-- Open Graph Tags -->
<%-
  open_graph({
    twitter_id: theme.twitter,
    google_plus: theme.google_plus,
    fb_admins: theme.fb_admins,
    fb_app_id: theme.fb_app_id,
    image: url_for('images/' + page.masthead)
  })
%>
<!-- End Open Graph Tags -->

<style>
.btn.btn-just-icon i, .navbar .navbar-nav > li > a.btn.btn-just-icon i {width:auto}
</style>
