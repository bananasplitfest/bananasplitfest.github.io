</head>

<body>
  <div class="wrapper">

    <!-- Begin Navigation -->
      <%- partial('_partial/layout-navigation.ejs') %>
    <!-- End Navigation -->

    <!-- Begin Body include -->
    <%- body %>
    <!-- End Body include -->

    <!-- Begin Footer -->
      <%- partial('_partial/layout-footer.ejs') %>
