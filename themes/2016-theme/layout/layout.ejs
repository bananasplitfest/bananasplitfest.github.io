    <%- js('js/material-kit.js') %>

    <!-- Plugin for InstaFeed, full documentation at http://instafeed.js -->
    <%- js('js/instafeed.min.js') %>
    <script type="text/javascript">
      var feed = new Instafeed({
          get: 'user',
          userId: '2111754365',
          clientId: '3237a68725e54852a7f3f7bb1a246a3b',
  				accessToken: '2111754365.3237a68.d56e80f70ff84bc8a36acf0628b6b13d',
  				limit: 8,
  				sortBy: 'random',
  				template: '<a href="{{link}}" target="_blank" /><img src="{{image}}" class="img img-raised img-rounded" alt="{{caption}}" /></a>'
      });
      feed.run();
    </script>

    <!-- Go to www.addthis.com/dashboard to customize your tools -->
